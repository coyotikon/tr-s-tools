print (Fore.BLUE + Style.BRIGHT+"[+] INSTALANDO POR FAVOR AGUARDE [+]"+'\033[1;30m')
def progress_bar(done):
    print(Fore.CYAN + Style.DIM+"\rCarregando: [{0:50s}] {1:.1f}%".format('#' * int(done * 50), done * 100),end='')

def test():
    for n in range(101):
        progress_bar(n/100)
        time.sleep(0.1)


test()      

